fun factorial(n) {
    if (n == 0) {
        return 1;
    }

    return n * factorial(n - 1);
}

fun fib(n) {
    if (n == 0 or n == 1) {
        return n;
    }

    return fib(n - 1) + fib(n - 2);
}


for (var i = factorial(5); i < 10; i += 1) {
    print(factorial(i));
